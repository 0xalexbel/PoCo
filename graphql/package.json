{
  "name": "iExecGraph",
  "version": "0.1.0",
  "scripts": {
    "build-contract": "solc contracts/Gravity.sol --abi -o abis --overwrite && solc contracts/Gravity.sol --bin -o bin --overwrite",
    "create": "graph create iExecBlockchainComputing/iExecGraph --node https://api.thegraph.com/deploy/",
    "create-local": "graph create iExecBlockchainComputing/iExecGraph --node http://127.0.0.1:8020",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy iExecBlockchainComputing/iExecGraph --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/",
    "deploy-local": "graph deploy iExecBlockchainComputing/iExecGraph --ipfs http://localhost:5001 --node http://127.0.0.1:8020"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.14.0",
    "@graphprotocol/graph-ts": "^0.14.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "iexec-solidity": "^0.0.14",
    "openzeppelin-solidity": "^2.3.0",
    "rlc-faucet-contract": "^1.0.8",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
